{
	"name":"Google Drive",
	"description":"Google Drive storage service provider.",
	"authorize_request":
	{
		"uri":"https://accounts.google.com/o/oauth2/auth",
		"method":"GET"
	},
	"token_request":
	{
		"uri":"https://accounts.google.com/o/oauth2/token",
		"method":"POST"
	},
	"grant_type":"AUTHORIZATION_CODE_GRANT",
	"client_id":"45396671053.apps.googleusercontent.com",
	"client_secret":"ZLa85_y8DSnMoEJ1IsyW3fmm",
	"redirect_uri":null,
	"scope":"https://www.googleapis.com/auth/drive",
	"root_folder":
	{
		"id":""
	},
	"account_info_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/about",
		"method":"GET",
		"mapping":
		{
			"id":"permissionId"
		}
	},
	"account_quota_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/about",
		"method":"GET",
		"mapping":
		{
			"totalBytes":"quotaBytesTotal",
			"usedBytes":"quotaBytesUsedAggregate"
		}
	},
	"download_file_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<id>",
		"method":"GET"
	},
	"upload_file_request":
	{
		"uri":"https://www.googleapis.com/upload/drive/v2/files",
		"method":"POST",
		"params":
		{
			"uploadType":"resumable"
		},
		"mapping":
		{
			"sessionId":"upload_id"
		},
		"headers":
		{
			"X-Upload-Content-Type":"application/octet-stream",
			"X-Upload-Content-Length":"<size>"
		}
	},
	"create_dir_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files",
		"method":"POST",
		"headers":
		{
			"Content-Type":"application/json"
		},
		"mapping":
		{
			"content":"items",
			"deleted":"labels/trashed",
			"size":"quotaBytesUsed",
			"name":"title"
		},
		"json_body":
		{
			"title":"<name>",
			"parents":
			[
				{
					"id":"<id>"
				}
			],
			"mimeType":"application/vnd.google-apps.folder"
		}
	},
	"list_dir_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<id>",
		"method":"GET",
		"params":
		{
			"trashed":"<deleted>"
		},
		"mapping":
		{
			"content":"items",
			"deleted":"labels/trashed",
			"size":"quotaBytesUsed",
			"name":"title"
		}
	},
	"rename_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<id>",
		"method":"PUT",
		"headers":
		{
			"Content-Type":"application/json"
		},
		"mapping":
		{
			"content":"items",
			"deleted":"labels/trashed",
			"size":"quotaBytesUsed",
			"name":"title"
		},
		"json_body":
		{
			"title":"<name>"
		}
	},	
	"copy_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<source_id>/copy",
		"method":"POST",
		"headers":
		{
			"Content-Type":"application/json"
		},
		"mapping":
		{
			"content":"items",
			"deleted":"labels/trashed",
			"size":"quotaBytesUsed",
			"name":"title"
		},
		"json_body":
		{
			"title":"<name>",
			"parents":
			[
				{
					"id":"<destination_id>"
				}
			]
		}
		
	},
	"move_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<source_id>",
		"method":"PUT",
		"headers":
		{
			"Content-Type":"application/json"
		},
		"mapping":
		{
			"content":"items",
			"deleted":"labels/trashed",
			"size":"quotaBytesUsed",
			"name":"title"
		},
		"json_body":
		{
			"parents":
			[
				{
					"id":"<destination_id>"
				}
			]
		}
	},
	"delete_request":
	{
		"uri":"https://www.googleapis.com/drive/v2/files/<id>",
		"method":"DELETE"
	}
}